230  1                 str            POS(0)
     1  +                             (BREAK('"' "'" QizeWierd) '"' ARBNO(NOTANY("'" QizeWierd))) . part
     1  +                             RTAB(0) . str                                         :S(Qize1)
231  1                 str            POS(0)
     1  +                             (BREAK("'" '"' QizeWierd) "'" ARBNO(NOTANY('"' QizeWierd))) . part
